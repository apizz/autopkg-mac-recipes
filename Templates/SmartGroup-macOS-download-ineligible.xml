<?xml version="1.0" encoding="UTF-8"?>
<computer_group>
  <name>%TEST_VERSION_INSTALLED_GROUP_NAME%</name>
  <is_smart>true</is_smart>
  <criteria>
    <criterion>
      <name>Operating System Version</name>
      <priority>0</priority>
      <and_or>and</and_or>
      <search_type>less than</search_type>
      <value>%MIN_OS_VERSION%</value>
    </criterion>
    <criterion>
      <name>Operating System Version</name>
      <priority>1</priority>
      <and_or>or</and_or>
      <search_type>greater than</search_type>
      <value>%MAX_OS_VERSION%</value>
    </criterion>
    <criterion>
      <name>Model Identifier</name>
      <priority>2</priority>
      <and_or>or</and_or>
      <search_type>does not match regex</search_type>
      <value>%MODEL_IDENTIFIER_REGEX%</value>
    </criterion>
  </criteria>
</computer_group>
